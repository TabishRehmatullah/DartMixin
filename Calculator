class Calculator with CalcPlus{
  divideInteger(x, y) => (x~/y);
  remainder(x, y) => (x%y);

} 

mixin CalcPlus{
  add(x, y) => (x+y);
  substract(x, y) => (x-y);
  multiply(x, y) => (x*y);
  divide(x, y) => (x/y);
}

void main(){
  Calculator calculator= Calculator();
  print(calculator.remainder(4,7));
}


  
